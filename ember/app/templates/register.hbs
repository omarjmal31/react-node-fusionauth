<div class="form-div">
    <div class="w-form w-clearfix form-wrapper">
        <form id="email-form" name="email-form" data-name="Email Form">
          {{#if errors.general}}
              <div>{{errors.general}}</div>
          {{/if}}
          {{input class="w-input" type="text" placeholder="Bob" value=first_name}}
          {{input class="w-input" type="text" placeholder="Smith" value=last_name}}
          {{input class="w-input" type="text" placeholder="example@email.com" value=email}}
          {{#if errors.email}}
              <div>{{errors.email}}</div>
          {{/if}}
          {{input class="w-input" type="password" placeholder="password" value=password}}
          {{#if errors.password}}
              <div>{{errors.password}}</div>
          {{/if}}
          {{input class="w-input" type="password" placeholder="password" value=confirm_password}}
          {{input class="w-input" type="checkbox" checked=two_factor}}
            <button class="w-button login-register-button" {{action "register"}}>Register</button>
            <button class="w-button login-register-button green" {{action "back"}}>Back</button>
        </form>
    </div>
</div>
